

    <!--Hero-->
    <div class="section no-pad-bot" id="index-banner">
        <div class="container">
            <h1 class="text_h center header cd-headline letters type">
                <span>I love</span>
                <span class="cd-words-wrapper waiting">
                    <b class="is-visible">learning</b>
                    <b>teaching</b>
                    <b>collaborating</b>
                </span>
            </h1>
        </div>
    </div>

    <!--Intro and service-->
    <div id="intro" class="section scrollspy">
        <div class="container">
            <div class="row">
                <div class="col s12">
                    <h2 class="center header text_h2">Finally, a<span class="span_h2"> single</span> resource for <span
                            class="span_h2"> all</span> of your development needs. </h2>
                </div>

                <div class="col s12 m4 l4">
                    <div class="center promo promo-example">
                        <i class="mdi-image-flash-on"></i>
                        <h5 class="promo-caption">Results Oriented Program</h5>
                        <p class="light center">Our full-stack web development course provides tips and shortcuts to
                            fast track your individual coding journey.</p>
                    </div>
                </div>
                <div class="col s12 m4 l4">
                    <div class="center promo promo-example">
                        <i class="mdi-social-group"></i>
                        <h5 class="promo-caption">Collaborative Experience</h5>
                        <p class="light center">Interactive forum for users and our team of TAs: ask questions, connect
                            with other students, and share resources.</p>
                    </div>
                </div>
                <div class="col s12 m4 l4">
                    <div class="center promo promo-example">
                        <i class="mdi-hardware-desktop-windows"></i>
                        <h5 class="promo-caption">Customized Learning</h5>
                        <p class="light center">Become a web developer at your own pace! Choose one of our recommended
                            course plans or curate your own curriculum.</p>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <!--Work-->
    <div class="section scrollspy" id="work">
        <div class="container">
            <h2 class="header text_b">Course Offerings </h2>
            <div class="row">
                {{#each course}}
                <div class="col s12 m4 l4" style="height:400px">
                    <div class="card" style="height: inherit">
                        <div class="card-image waves-effect waves-block waves-light" style="height: inherit">
                            <img class="activator" src="/upload-images/{{dataValues.course_image}}" style="height: inherit">
                        </div>
                        <div class="card-content">
                            <span class="card-title activator grey-text text-darken-4">{{dataValues.course_title}} <i
                                    class="mdi-navigation-more-vert right"></i></span>
                            <p><a href="/student-course/{{dataValues.cid}}">GO TO COURSE</a></p>
                        </div>
                        <div class="card-reveal">
                            <span class="card-title grey-text text-darken-4">{{dataValues.course_title}} <i
                                    class="mdi-navigation-close right"></i></span>
                            <p>{{dataValues.course_description}}
                            </p>
                        </div>
                    </div>
                </div>
                {{/each}}
               
            </div>
        </div>
    </div>

    <!--Parallax-->
    <div class="parallax-container">
        <div class="parallax"><img src="styles/img/parallax1.png"></div>
    </div>

